            <Origin>EVENT</Origin>
        </Explanation>
        <Explanation columnName="EVENT_START_DT_TM">
            <Description>Lab test event start datetime</Description>
            <Origin>EVENT START DT TM</Origin>
        </Explanation>
        <Explanation columnName="RESULT_VALUE">
