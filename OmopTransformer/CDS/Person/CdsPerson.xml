<Query>
    <Sql>
select
	distinct
		NHSNumber,
		DateofBirth as DateOfBirth,
		EthnicCategory,
		PersonCurrentGenderCode
from omop_staging.cds_line01
where NHSNumber is not null;
	</Sql>
    <Explanation>
		<OmopColumnExplanation columnName="NHSNumber">Patient NHS Number</OmopColumnExplanation>
		<OmopColumnExplanation columnName="DateOfBirth">Patient DateOfBirth</OmopColumnExplanation>
		<OmopColumnExplanation columnName="EthnicCategory">Patient EthnicCategory [Data Dictionary](https://www.datadictionary.nhs.uk/data_elements/ethnic_category.html)</OmopColumnExplanation>
		<OmopColumnExplanation columnName="PersonCurrentGenderCode">Patient PersonCurrentGenderCode [Data Dictionary](https://www.datadictionary.nhs.uk/data_elements/person_gender_code_current.html)</OmopColumnExplanation>
	</Explanation>
</Query>